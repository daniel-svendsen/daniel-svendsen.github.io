{
  "compilerOptions": {
    "strict": false,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "@cloudflare/workers-types",
      "node"
    ]
  },
  "include": [
    "src"
  ]
}